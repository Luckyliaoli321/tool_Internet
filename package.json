{
  "name": "tool-website",
  "version": "0.1.0",
  "description": "A website providing various file and image processing tools",
  "scripts": {
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "start-frontend": "cd frontend && cross-env PORT=3002 npm start",
    "start-backend": "cd backend && npm start",
    "dev-backend": "cd backend && npm run dev",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "antd": "^5.24.3",
    "axios": "^1.8.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^24.4.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.3.0",
    "sharp": "^0.33.5",
    "uuid": "^11.1.0"
  }
}
